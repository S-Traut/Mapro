<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Mapro
			{% endblock %}
		</title>

		<script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>

		<!-- Bootstrap CSS build -->
		{{ encore_entry_link_tags('app') }}

		<!-- Viewport mobile -->
		<meta name="viewport" content="width=device-width, initial-scale=1.0"> {% block stylesheets %}{% endblock %}
		<link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.min.css"/> {% block headScripts %}{% endblock %}
		</head>
		<body>
			{% include 'header.html.twig' %}
