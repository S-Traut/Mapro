			<script src="https://kit.fontawesome.com/8245776c75.js" crossorigin="anonymous"></script>
			<script src="https://unpkg.com/swiper/swiper-bundle.min.js"></script>
			{{ encore_entry_script_tags('favoris') }}
			{% block javascripts %}
			{% endblock %}
			{% include "footer.html.twig" %}
		</body>
	</html>
